{
  "private": true,
  "scripts": {
    "script": "ts-node scripts",
    "precodegen": "rimraf src/generated src/mappings/feeds/*.ts && yarn script graphgen",
    "codegen": "graph codegen subgraph.yaml --output-dir src/generated",
    "postcodegen": "yarn script flatten",
    "deploy": "graph deploy --studio chainlink-mainnet",
    "precodegen:arbitrum": "rimraf src/generated src/mappings/feeds/*.ts && yarn script graphgen --network arbitrum-one",
    "codegen:arbitrum": "graph codegen subgraph.yaml --output-dir src/generated",
    "postcodegen:arbitrum": "yarn script flatten",
    "precodegen:mainnet": "rimraf src/generated src/mappings/feeds/*.ts && yarn script graphgen --network mainnet",
    "codegen:mainnet": "graph codegen subgraph.yaml --output-dir src/generated",
    "postcodegen:mainnet": "yarn script flatten",
    "precodegen:polygon": "rimraf src/generated src/mappings/feeds/*.ts && yarn script graphgen --network matic",
    "codegen:polygon": "graph codegen subgraph.yaml --output-dir src/generated",
    "postcodegen:polygon": "yarn script flatten",
    "deploy:arbitrum": "graph deploy --studio chainlink-arbitrum",
    "format": "prettier --write {src,scripts}/{*,**/*}.{ts,json,yaml}",
    "lint": "yarn prettier --list-different {src,scripts}/{*,**/*}.{ts,json,yaml}"
  },
  "devDependencies": {
    "@chainlink/contracts": "^0.2.1",
    "@graphprotocol/graph-cli": "0.21.0",
    "@graphprotocol/graph-ts": "^0.19.0",
    "@types/mustache": "^4.0.1",
    "@types/node": "^14.14.3",
    "@types/prettier": "^2.1.5",
    "@types/rimraf": "^3.0.0",
    "@types/yargs": "^15.0.9",
    "assemblyscript": "^0.17.0",
    "glob": "^7.1.6",
    "jest": "^26.6.1",
    "mustache": "^4.0.1",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "yargs": "^16.1.0"
  }
}
